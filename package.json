{
  "name": "ts-template",
  "version": "1.0.0",
  "description": "Template for typescript node projects",
  "main": "dist/app.js",
  "directories": {},
  "scripts": {
    "build": "rimraf dist && tsc",
    "build-all": "npm run build;",
    "clean": "rimraf dist",
    "cspell": "cspell lint src/**/*.ts",
    "start": "node dist/app.js",
    "test": "jest",
    "clean-jest": "jest --clear-cache",
    "coverage": "jest --coverage",
    "lint": "eslint src test/unit --ext .ts",
    "fixlint": "eslint src test/unit --ext .ts --fix"
  },
  "keywords": [
    "typescript",
    "node"
  ],
  "author": "Andrew Xu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrxu/ts-template/issues"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^20.11.25",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "cspell": "^8.5.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.4.3",
    "prettier": "^2.8.4",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  }
}
